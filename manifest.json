{
   "background": {
      "scripts": ["js/jquery-2.0.3.min.js", "js/utils.js", "js/background.js"],
      "persistent": false
   },
   "browser_action": {
      "default_icon": "images/19.png",
      "default_popup": "popup.html",
      "default_title": "TV Countdown"
   },
   "description": "Count down the days, hours, minutes and seconds left until your favorite shows air.",
   "icons": {
      "128": "images/128.png",
      "16": "images/16.png",
      "48": "images/48.png"
   },
   "author": "Xxxxx",
   "name": "TV Countdown",
   "options_page": "options.html",
   "version": "3.0.0.3",
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "manifest_version": 2,
   "permissions": [
      "https://tvcountdown.com/*",
	  "storage"
   ]
}
